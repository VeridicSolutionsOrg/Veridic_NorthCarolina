                sh 'echo "this is build step"' 
            }
        }
        stage('Test'){
            steps {
                sh 'echo "this is test step"'
                           }
        }
        stage('Deploy') {
            steps {
                sh 'echo "this is deploy step"'
            }
        }
    }
}
