#!/usr/bin/env groovy
pipeline {
    agent any 

    stages {
        stage('Checkout Stage1'){
            steps {
                echo 'pwd'
            }
        }
        stage('Build Stage') {
            steps {
                echo 'cd /etc'
            }
        }
        stage('Starting database rebuild'){
            steps {
                 echo 'pwd'
            }
        }
    }
}
