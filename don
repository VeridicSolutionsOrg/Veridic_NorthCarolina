pipeline {
    agent any 

    stages {
        stage('Build') { 
            steps {
sh 'echo "I am don"'  
            }
        }
        stage('Test'){
            steps {
                sh 'echo "this is test"'
                
            }
        }
        stage('Deploy') {
            steps {
                sh 'echo "this is Deploy stage"'
            }
        }
    }
}
